@model Shampan.Models.DeshboardSettings

@{
    ViewData["Title"] = "Index";
}

@Html.Partial("_Header",new HeaderModel(){ HeaderName = "Deshboard Setting", BreadCrums = new Dictionary<string, string>()
{
    { "/UserRolls","Branch Profile"},
    { "","Index"},
}})

<link href="~/css/button.css" rel="stylesheet" />

<div class="container-fluid">

    <div class="card">
        <div class="card-header p-2">
            <button type="button" title="Add New BankEntry"
                    data-url="/Deshboard/DeshboardSettingCreate" onclick="GoTo(this)"
                    id="" class="button NewButton addNewButton">
                New
            </button>
        </div>
    </div>


    <div class="card card-secondary">

        <div class="card-header">
            <h3 class="card-title">
                List Of Data
            </h3>
        </div>

        <div class="card-body">
            <div class="table-responsive">

                <table class="table table-bordered table-striped" style="width:100%" id="DeshboardSettingList">
                    <thead>
                        <tr>

                            <th class="action">&nbsp;Action</th>
                            <th>User Name</th>
                            <th class="bool">Pie Chart</th>
                            <th class="bool">Planned Engagement</th>
                            <th class="bool">Unplanned Engagement</th>
                            <th class="bool">Year Data</th>
                            <th class="bool">Box Data</th>

                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>

@section scripts {

    <script src="/js/app/Services/CommonService.js"></script>
    <script src="/js/app/Services/DeshboardEntryService.js"></script>
    <script src="/js/app/Controllers/DeshboardEntryController.js"></script>

    <script>
        $(document).ready(function () {
            DeshboardEntryController.init();
        });
    </script>

}

