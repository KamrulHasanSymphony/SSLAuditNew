@model Shampan.Models.FiscalYearVM
@{
    ViewBag.Title = "Fiscal Year Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"> Fiscal Year List </h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    
                </ol>
            </div>
        </div>
    </div>
</div>


<div class="container-fluid">
    <div class="card">
        <div class="card-header p-2">
            <button type="button" title="Add New" data-url="/FiscalYear/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                New
            </button>

            @* <button type="button" title="Delete" class="button sslUnPost btnDelete"> Delete </button> *@

        </div>
    </div>
</div>


<div class="card card-secondary">
    <div class="card-header">
        <h3 class="card-title">
            List Of Data
        </h3>

    </div>
    <div class="card-body">
        <div class="table-responsive">

            <table class="table table-bordered table-striped" style="width:100%" id="FiscalYearList">
                <thead>
                    <tr>

                        <th class="action"><input type="checkbox" class="chkAll" />&nbsp;Action</th>
                        <th>Year</th>
                        <th>Year Start</th>
                        <th>Year End</th>

                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>


@section scripts{

    <script src="/js/app/Services/CommonService.js"></script>
    <script src="/js/app/Services/FiscalYearsService.js"></script>
    <script src="/js/app/Controllers/FiscalYearsController.js"></script>

    <script>
        $(document).ready(function () {

            FiscalYearsController.init();
        });

    </script>


}


